<div id="wrapper">
  {{#if session.isAuthenticated}}
    {{app-menu logout="invalidateSession"}}

    <!-- Page Content -->
    <div id="page-wrapper">
      <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12">

            <div class="flash-container">

            {{#liquid-spacer}}
              {{#each flashMessages.queue as |flash|}}

                {{flash-message flash=flash}}
              {{/each}}
            {{/liquid-spacer}}
            </div>

            {{liquid-outlet}}
          </div>
          <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </div>
  {{else}}
    {{liquid-outlet}}
  {{/if}}
  <!-- /#page-wrapper -->
</div>
